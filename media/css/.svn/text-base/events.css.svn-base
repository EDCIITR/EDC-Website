a.flagship
{
	display:block;
	float:left;
	background-color:#fafafa;
	border-color:#009900;
	border-style:solid;
	border-width:1px;
	width:100%;
	padding:5px;
	margin-top:10px;
}

a.flagship:hover
{
	border-color:#ffcc00;
	text-decoration:none;
}

#form
{
	border:4px;
	border-style:solid;
	border-color:#ffcc00;
	
}

#container
{
	height:100%;
}

#labelContainer
{
	margin-left:40px;
	display:block;
	height:34px;
}

#label
{
	background-color:#e6e6e6;
	display:inline-block;
	padding:10px 10px 10px 10px; 
	text-align:center;
}

#label.active
{
	background-color:#ffcc00;
}
